<h2 class="text-2xl font-bold text-gray-800 mb-6">Upload Feedbacks CSV</h2>

<%= form_with url: upload_csv_feedbacks_path,
              method: :post,
              html: { multipart: true },
              data: { turbo: false },
              class: "space-y-4" do |form| %>

  <div>
    <%= form.label :file, "Choose CSV file", class: "text-sm font-medium text-gray-700" %>
    <%= form.file_field :file,
        class: "py-2 px-4 bg-blue-200 rounded w-fit" %>
  </div>

  <%= form.submit "Upload",
      class: "py-2 px-4 rounded bg-green-500 rounded w-fit text-white hover:text-gray-900" %>
<% end %>
